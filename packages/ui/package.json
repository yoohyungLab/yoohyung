{
    "name": "@typologylab/ui",
    "version": "0.0.0",
    "private": true,
    "type": "module",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "types": "./dist/index.d.ts"
        },
        "./dist/styles.css": "./dist/styles.css"
