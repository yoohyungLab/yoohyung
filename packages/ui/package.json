        "lucide-react": "^0.522.0",
        "react-hook-form": "^7.60.0",
        "tailwind-merge": "^3.3.1",
        "vaul": "^1.1.2"
    },
    "devDependencies": {
        "@types/react": "^19.1.8",
        "@types/react-dom": "^19.1.6",
        "autoprefixer": "^10.4.16",
        "postcss": "^8.4.32",
        "tailwindcss": "^3.4.0",
