{
	"buildCommand": "cd ../.. && pnpm build --filter=pickid-admin",
	"outputDirectory": "dist",
	"installCommand": "cd ../.. && pnpm install",
	"framework": "vite",
	"rewrites": [
		{
			"source": "/((?!api/).*)",
			"destination": "/index.html"
		}
	],
	"headers": [
		{
			"source": "/static/(.*)",
			"headers": [
				{
					"key": "Cache-Control",
					"value": "public, max-age=31536000, immutable"
				}
			]
		}
	]
}
