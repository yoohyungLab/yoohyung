{
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": ["dist/**", ".next/**", "build/**"]
		},
		"dev": {
			"cache": false,
			"persistent": true
		},
		"lint": {},
		"test": {
			"dependsOn": ["^build"]
		},
		"type-check": {
			"dependsOn": ["^build"]
		},
		"clean": {
			"cache": false
		}
	}
}
